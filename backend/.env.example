# Database Configuration
DB_HOST=localhost
DB_PORT=1521
DB_SERVICE=your_service_name
DB_USER=your_username
DB_PASSWORD=your_password

# Oracle Client (optional, for thick mode)
ORACLE_HOME=

# LLM Configuration
GROQ_API_KEY=your_groq_api_key_here
GROQ_MODEL=qwen-2.5-32b

# Application Settings
DEBUG=False
LOG_LEVEL=INFO

# Query Limits
MAX_ROWS=2000
SQL_RETRY_ATTEMPTS=3

# Dictionary Path (leave empty to use default)
DICTIONARY_PATH=

# CORS Settings (comma-separated origins, or * for all)
CORS_ORIGINS=*
